<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://java.sun.com/xml/ns/javaee" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" version="2.5">
  <display-name>MovingAboutMaven</display-name>
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
  <servlet>
    <description></description>
    <display-name>DirectServlet</display-name>
    <servlet-name>DirectServlet</servlet-name>
    <servlet-class>com.revature.servlets.DirectServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>DirectServlet</servlet-name>
    <url-pattern>/DirectServlet</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>DirectServlet</servlet-name>
    <url-pattern>/SuperDirect</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>DirectServlet</servlet-name>
    <url-pattern>/DirectReloaded</url-pattern>
  </servlet-mapping>
  <servlet>
    <description></description>
    <display-name>IndirectServlet</display-name>
    <servlet-name>IndirectServlet</servlet-name>
    <servlet-class>com.revature.servlets.IndirectServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>IndirectServlet</servlet-name>
    <url-pattern>/IndirectServlet</url-pattern>
  </servlet-mapping>
 
  <servlet-mapping>
    <servlet-name>CookieServlet</servlet-name>
    <url-pattern>/CookieServlet</url-pattern>
  </servlet-mapping>

  <servlet-mapping>
    <servlet-name>SessionServlet</servlet-name>
    <url-pattern>/SessionServlet</url-pattern>
  </servlet-mapping>

</web-app>